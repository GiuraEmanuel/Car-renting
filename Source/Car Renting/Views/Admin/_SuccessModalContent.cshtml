@model SuccessModalContentViewModel

<div class="modal-header">
    <h5 class="modal-title" id="editVehicleModalLabel">Success!</h5>
    <button type="button" class="close" onClick="window.location.href=window.location.href" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    @Model.Message
</div>
<div class="modal-footer" >
    <button class="btn btn-primary" data-dismiss="modal" onClick="window.location.href=window.location.href">Close</button>
</div>
